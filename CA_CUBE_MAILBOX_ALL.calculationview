</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="OK_COMPLIANCE" expressionLanguage="COLUMN_ENGINE">
          <formula>if('$$onedrive_compliant$$'='checked'  ,if(&quot;ONEDRIVE_QUALITY&quot;='OK',1,0),0)+&#xD;
if('$$sharepoint_compliant$$'='checked'  ,if(&quot;SHAREPOINT_QUALITY&quot;='OK',1,0),0)+&#xD;
if('$$skype_compliant$$'='checked' ,if(&quot;SKYPE_QUALITY&quot;='OK',1,0),0)+&#xD;
if('$$yammer_compliant$$'='checked'  ,if(&quot;YAMMER_QUALITY&quot;='OK',1,0),0)+&#xD;
if('$$teams_compliant$$'='checked'  ,if(&quot;TEAMS_QUALITY&quot;='OK',1,0),0)&#xD;
&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="NOK_COMPLIANCE" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;TOP_GRADE&quot;-&quot;OK_COMPLIANCE&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="COUNT_COMPLIANCE" expressionLanguage="COLUMN_ENGINE">
          <formula>1</formula>
        </calculatedViewAttribute>
