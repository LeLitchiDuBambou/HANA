        <calculatedViewAttribute datatype="INTEGER" id="TOP_GRADE" expressionLanguage="COLUMN_ENGINE">
          <formula>if('$$onedrive_compliant$$'='checked',1,0)+&#xD;
if( '$$sharepoint_compliant$$'='checked',1,0) +&#xD;
if ('$$skype_compliant$$'='checked',1,0)+&#xD;
if( '$$yammer_compliant$$'='checked',1,0)+&#xD;
if('$$teams_compliant$$'='checked',1,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="ONEDRIVE_QUALITY" length="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if( isnull(&quot;USER_ACTIVITY_ONEDRIVE&quot;) or &quot;USER_ACTIVITY_ONEDRIVE&quot;&lt;$$activite_minimum_requise$$,'KO','OK')&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="SHAREPOINT_QUALITY" length="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isnull(&quot;USER_ACTIVITY_SHAREPOINT&quot;) or &quot;USER_ACTIVITY_SHAREPOINT&quot;&lt;$$activite_minimum_requise$$,'KO','OK')&#xD;
</formula>
        </calculatedViewAttribute>
